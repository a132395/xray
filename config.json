{
  "log": {
    "access": "/var/log/xray/access.log",
    "error": "/var/log/xray/error.log",
    "loglevel": "error"
  },
 
  "inbounds": [
   {
    "port": 50000,
    "listen": "127.0.0.1",
    "protocol": "vless",
    "settings": {
      "clients": [
        {
          "id": "00000000-0000-0000-0000-000000000000",
          "level": 0,
        }
      ],
      "decryption": "none"
   },  
    "streamSettings": {
      "network": "ws",
      "wsSettings": {
        "connectionReuse": true,
        "path": "/api/"
       }
      }  
    }
  ],
  "outbounds": [{
    "protocol": "freedom",
    "settings": {}
  },{
    "protocol": "blackhole",
    "settings": {},
    "tag": "blocked"
  }],
  "routing": {
    "rules": [
      {
        "type": "field",
        "ip": ["geoip:private"],
        "outboundTag": "blocked"
      }
    ]
  }
}
